{
  "expo": {
    "name": "BookExplorer",
    "slug": "book-explorer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.bookexplorer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourname.bookexplorer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
"plugins": [
  [
    "expo-font",
    {
      "fonts": [
        "./src/assets/fonts/Helvetica.ttf",
        "./src/assets/fonts/Roboto-Regular.ttf"
      ]
    }
  ]
],
    "extra": {
      "eas": {
        "projectId": "d0fa81e0-82f0-471d-8f44-a0f399f8b9b3"
      }
    }
  }
}
